{
  "name": "nx-helpers",
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/master/packages/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "tools/@o3r/nx-helpers/src",
  "prefix": "o3r",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "outputs": [
        "{projectRoot}/dist/package.json"
      ],
      "inputs": [
        "base",
        "{projectRoot}/package.json"
      ],
      "options": {
        "script": "postbuild"
      },
      "dependsOn": [
        "compile"
      ]
    },
    "compile": {
      "executor": "nx:run-script",
      "options": {
        "script": "build"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "tools/@o3r/nx-helpers/jest.config.js"
      }
    }
  },
  "tags": []
}
